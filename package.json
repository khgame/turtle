{
  "name": "@khgame/turtle",
  "version": "0.0.78",
  "description": "An enterprise level javascript/typescript server framework.",
  "keywords": [
    "web",
    "application",
    "microservice",
    "distributed",
    "cluster",
    "middleware",
    "driver",
    "plugin",
    "framework"
